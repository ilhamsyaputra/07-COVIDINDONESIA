def plot_bar(df, title):
    df.plot(
        kind='bar',
        x='key',
        y='doc_count',
        figsize=(12,6),
        title=title,
        legend=False,
        xlabel='',
        ylabel='Persen (%)',
        rot=45,
    )

def plot_pie(df, title):
    df.plot(
        kind='pie',
        x='key',
        y='doc_count',
        figsize=(12,6),
        title=title,
        ylabel='',
        legend=False,
        autopct='%1.1f%%'
    )





